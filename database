DROP TABLE IF EXISTS tokenLog CASCADE;
--DROP TABLE IF EXISTS TokenLog CASCADE;
CREATE TABLE tokenLog(
id serial PRIMARY KEY,
usuario VARCHAR(50),
anteriorToken VARCHAR(500),
actualToken VARCHAR(500),
mensaje VARCHAR(500),
CONSTRAINT fk_ususario FOREIGN KEY (usuario) REFERENCES Usuario(usuario));
insert into tokenLog(usuario,anteriorToken,actualToken,mensaje) values ('joeaespi', ' ', '132A','Ha cambiado su tokena a 123A');
select * from TokenLog;
